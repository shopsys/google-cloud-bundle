{
    "name": "shopsys/google-cloud-bundle",
    "type": "library",
    "description": "Shopsys Framework Google Cloud Bundle",
    "keywords": ["google cloud", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\GoogleCloudBundle\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "google/cloud-storage": "^1.10",
        "shopsys/form-types-bundle": "dev-tp-symfony4 as dev-master",
        "shopsys/framework": "dev-tp-symfony4 as dev-master",
        "shopsys/migrations": "dev-tp-symfony4 as dev-master",
        "shopsys/plugin-interface": "dev-tp-symfony4 as dev-master",
        "superbalist/flysystem-google-storage": "^7.1",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/options-resolver": "^4.4"
    },
    "require-dev": {
        "shopsys/coding-standards": "dev-tp-symfony4 as dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0.x-dev"
        }
    }
}
